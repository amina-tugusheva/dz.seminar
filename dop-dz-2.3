#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int s = 0, pr = 1;
	int n, a;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		cin >> a;
		if ((a > 0) && (a % 2 == 0))
		{
			s += a;
			pr *= a;
		}
	}
	cout << "сумма = " << s << endl;
	cout << "произведение = " << pr << endl;
}
